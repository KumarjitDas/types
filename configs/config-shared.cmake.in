@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/@KDAPI_PROJECT_NAME_LOWER@-targets.cmake")

set(@KDAPI_LIBRARY_NAME_UPPER@_VERSION_MAJOR @KDAPI_VERSION_MAJOR@)
set(@KDAPI_LIBRARY_NAME_UPPER@_VERSION_MINOR @KDAPI_VERSION_MINOR@)
set(@KDAPI_LIBRARY_NAME_UPPER@_VERSION_PATCH @KDAPI_VERSION_PATCH@)
set(@KDAPI_LIBRARY_NAME_UPPER@_VERSION @KDAPI_VERSION@)

set_and_check(@KDAPI_LIBRARY_NAME_UPPER@_LIB_DIR     "@PACKAGE_KDAPI_INSTALL_LIB_DIR@")
set_and_check(@KDAPI_LIBRARY_NAME_UPPER@_BIN_DIR     "@PACKAGE_KDAPI_INSTALL_BIN_DIR@")
set_and_check(@KDAPI_LIBRARY_NAME_UPPER@_INCLUDE_DIR "@PACKAGE_KDAPI_INSTALL_INCLUDE_DIR@")
set_and_check(@KDAPI_LIBRARY_NAME_UPPER@_CMAKE_DIR   "${@KDAPI_LIBRARY_NAME_UPPER@_LIB_DIR}/@KDAPI_INSTALL_CMAKE_DIR@")

check_required_components(@KDAPI_PROJECT_NAME@)
